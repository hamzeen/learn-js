{
  "gitbook": "3.1.2",
  "title": "Learn JS",
  "plugins": [
    "include-codeblock",
    "advanced-emoji",
    "prism",
    "highlight",
    "copy-code-button",
    "edit-link",
    "github",
    "github-buttons",
    "ga"
  ],
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/hamzeen/learn-js/tree/main",
      "label": "Edit This Page"
    },
    "github": {
      "url": "https://github.com/hamzeen/learn-js/"
    },
    "ga": {
      "token": "UA-54001708-2"
    },
    "github-buttons": {
      "buttons": [
        {
          "user": "hamzeen",
          "repo": "learn-js",
          "type": "star",
          "size": "small",
          "count": true
        },
        {
          "user": "hamzeen",
          "repo": "learn-js",
          "type": "watch",
          "size": "small",
          "count": true
        }
      ]
    }
  }
}
